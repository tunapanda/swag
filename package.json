{
  "name": "swag",
  "version": "0.1.0",
  "main": "server/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "cypress run --record --key d9ab3f02-3f5f-4f23-9064-cec36274c15d",
    "cy:open": "cypress open",
    "test:server": "yarn --cwd ./server test",
    "test:frontend": "yarn --cwd ./frontend test",
    "test:all": "concurrently yarn:test yarn:test:frontend yarn:test:server",
    "build:server": "yarn --cwd ./server",
    "build:frontend": "yarn --cwd ./frontend && yarn --cwd ./frontend build --prod",
    "start:frontend": "yarn --cwd ./frontend start:local",
    "start:server": "nodemon ./server index.js",
    "docs": "apidoc -e node_modules -e docs -o docs/"
  },
  "lint-staged": {
    "frontend/*.js": "yarn --cwd ./frontend lint:js --fix",
    "frontend/*.hbs": "yarn --cwd ./frontend lint:hbs --fix",
    "server/*js": "yarn --cwd ./frontend lint:hbs --fix"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.1",
    "all-contributors-cli": "^6.17.2",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^6.2.1",
    "eslint-plugin-cypress": "^2.6.1",
    "husky": "^5.0.9",
    "istanbul-lib-coverage": "^3.0.0",
    "knex": "^0.21.15",
    "nyc": "^15.0.0",
    "pg": "^8.3.3"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "eslint-plugin-ember": "^8.10.1",
    "eslint-plugin-node": "^11.1.0",
    "lint-staged": "^10.5.4",
    "redis": "^3.0.2"
  }
}
