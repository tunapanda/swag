<div id="mySidenav" class="sidenav">
  <button class="closebtn" {{action "closenav"}}>&times;</button>
  {{!-- {{#link-to class="closebtn" invokeAction=(action "closenav") }}
    &times;
  {{/link-to}} --}}
  {{#if me.user.id}}
    {{#link-to "index" invokeAction=(action "closenav")  }}
      Home
    {{/link-to}}
    {{#link-to "home" invokeAction=(action "closenav")  }}
      My Courses

    {{/link-to}}

    {{#link-to "courses" invokeAction=(action "closenav")  }}
      Explore Courses

    {{/link-to}}
    {{#link-to "profile" me.user.username invokeAction=(action "closenav")  }}
      Profile

    {{/link-to}}
    <a href="#" {{ action "logout" }}>Logout</a>
    {{#link-to "profile" me.user.username (query-params view="Settings")
  invokeAction=(action "closenav")  }}
      Settings

    {{/link-to}}
